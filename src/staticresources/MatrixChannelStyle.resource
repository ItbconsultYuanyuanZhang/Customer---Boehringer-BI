
.ui-widget { font-family: Trebuchet MS,Tahoma,Verdana,Arial,sans-serif; font-size: 1.1em; }
.ui-widget-content { border: 1px solid #dddddd; background: #eeeeee; color: #333333; }
.ui-corner-all, .ui-corner-left{ -moz-border-radius-topleft: 4px; -webkit-border-top-left-radius: 4px; -khtml-border-top-left-radius: 4px; border-top-left-radius: 4px; }
.ui-widget-header { border: 1px solid #e78f08; background: #f6a828; color: #ffffff; font-weight: bold; }
.ui-progressbar { height:2em; text-align: left; overflow: hidden; }
.ui-progressbar .ui-progressbar-value {margin: -1px; height:100%; }
.tac{text-align: center !important;}
.tar{text-align: right !important;}
.fw{font-weight: bold;}
.matrixoverCls{}
.dropTarget{
	-moz-border-radius: 4px; -webkit-border-radius: 4px; border-radius:4px;
	background-color:#E6F5FB !important;opacity:0.65; z-index:8000;filter:alpha(opacity=65);background-repeat:no-repeat;background-position:center center;
}
.hoverTarget{ background-color:#e4e4ef !important; }
.clearfix:after {
	content: ".";
	clear: both;
	height: 0;
	visibility: hidden;
	display: block;
}
.startdiv{text-align: center !important;margin: 0 auto;width: 800px;vertical-align: middle;}
.hdvhc {
	white-space: nowrap;
	-webkit-transform: rotate(-90deg);
	-moz-transform: rotate(-90deg);
	-o-transform: rotate(-90deg);
	-ms-transform: rotate(-90deg);
	transform: rotate(-90deg);
	display: block;
	/*margin-top: 150px;*//*commented by Peng Zhu 2013-05-23*/
	font-weight: bold;
	position: relative;
	filter: progid:DXImageTransform.Microsoft.BasicImage(rotation=3);
	-ms-filter: "progid:DXImageTransform.Microsoft.BasicImage(rotation=3)";
}

