.channelCls{position: relative; padding:1px; border: 1px solid #999;-moz-border-radius: 4px; -webkit-border-radius: 4px; border-radius:4px;}
.channelCls div{width: 100%; position: relative; vertical-align: middle;}
.channelCls div.h1{ height: 40px; line-height: 40px; -moz-border-radius: 4px; -webkit-border-radius: 4px; border-radius:4px;}
.channelCls div.h2{ 
	height: 16px; line-height: 16px;
	-moz-border-radius: 4px;      /* Gecko browsers */
	-webkit-border-radius: 4px;   /* Webkit browsers */
	border-radius:4px;            /* W3C syntax */
	overflow: hidden;
}
.channelCls div.h2 span.sCls{vertical-align: middle;position: relative;display: block;height: 100%;line-height: 100%;}
.channelTrDataH td div.heightCls{height: 60px; line-height: 60px;min-height: 60px; max-height: 60px;}

.matrixCls{ overflow: hidden; margin-top: 10px; width: 100%;}

.matrixCls div.matrixDivLabel{padding-top:26px; float: left; position: relative; width:100%; word-break:break-all;}
.matrixCls div.matrixDivLabel  ul{overflow:hidden;list-style:none; position: relative; float: right; margin: 0; padding: 0;}
.matrixCls div.matrixDivLabel  ul li{float:right;display:inline; word-wrap:break-word;word-break:nomal; padding: 0; -moz-border-radius: 4px; -webkit-border-radius: 4px; border-radius:4px;}

.matrixCls div.labelCls{position: relative; height: 100%; line-height: 100%; float: left;width: 26px;overflow: hidden;}
.matrixCls div.matrixDivTable {float: left; width: 97%;}
.matrixCls div.matrixDivTable h1.hCls{}

.matrixTotalCls{overflow: hidden; margin-top:2px; width: 1160px; padding-left:30px; float:left;}
.matrixTotalCls div{position: relative;}
.matrixTotalCls div.matrixDivLabel{ height: 30px; line-height: 30px;text-align: left; width: 100%;}

.matrixTotalCls div.matrixDivTable{*float:left;}


.matrixTabTotalCls {text-align: right; width:100% !important;}
.matrixTabTotalCls th.brt,td.brt{text-align:right; border-right: 1px solid #e0e3e5 !important; border-left: none !important; border-bottom: 1px solid #f2f2f3 !important; }
.matrixTabTotalCls th.nbrt, td.nbrt {border-right: none !important;}
.matrixTabTotalCls th.nbbt, td.nbbt {border-bottom: none !important;}

.matrixTabCls {text-align: right;}
.matrixTabCls th.brt,td.brt{border-right: 1px solid #e0e3e5 !important; border-left: none !important; border-bottom: 1px solid #f2f2f3 !important; }
.matrixTabCls th.nbrt, td.nbrt {border-right: none !important;}
.matrixTabCls th.nbbt, td.nbbt {border-bottom: none !important;}

