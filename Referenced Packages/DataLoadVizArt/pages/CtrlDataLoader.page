            if(hr<10) hr = '0'+hr;
            if(mi<10) mi = '0'+mi;
            if(se<10) se = '0'+se;
            if(ms < 10) ms = '00'+ms;
            else if(ms<100) ms = '0'+ms;
            return hr+':'+mi+':'+se+'.'+ms;
